#include <stdio.h>

int main() {
   int amount, notes;

   printf("Enter an amount: ");
   scanf("%d", &amount);

   printf("Total number of notes:\n");

   notes = amount / 500;
   printf("500: %d\n", notes);
   amount = amount % 500;

   notes = amount / 100;
   printf("100: %d\n", notes);
   amount = amount % 100;

   notes = amount / 50;
   printf("50: %d\n", notes);
   amount = amount % 50;

   notes = amount / 20;
   printf("20: %d\n", notes);
   amount = amount % 20;

   notes = amount / 10;
   printf("10: %d\n", notes);
   amount = amount % 10;

   notes = amount / 5;
   printf("5: %d\n", notes);
   amount = amount % 5;

   notes = amount / 2;
   printf("2: %d\n", notes);
   amount = amount % 2;

   notes = amount / 1;
   printf("1: %d\n", notes);

   return 0;
}
